{
    "name": "mbonaldo/comingsoon",
    "description": "Handle Coming Soon page",
    "license": "MIT",
    "authors": [
        {
            "name": "Mauro Bonaldo",
            "email": "mbonaldo@gmail.com",
            "homepage": "http://www.bonaldo.cloud/"
        }	
    ],
    "homepage": "https://github.com/CloudDelphi/comingsoon",
    "keywords": ["Laravel", "ComingSoon"],
    "require": {
        "illuminate/support": "~5|~6|~7"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3|~4",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "MBonaldo\\ComingSoon\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MBonaldo\\ComingSoon\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MBonaldo\\ComingSoon\\ComingSoonServiceProvider"
            ],
            "aliases": {
                "ComingSoon": "MBonaldo\\ComingSoon\\Facades\\ComingSoon"
            }
        }
    }
}
